{
	"default_module": "",
	"segments": {
		"right": [
			{
				"function": "powerline.segments.common.wthr.weather",
				"priority": 50,
        "args": {
          "unit": "F",
          "temp_coldest": -10,
          "temp_hottest": 40
        }
			},
			{
				"function": "powerline.segments.common.time.date"
			},
			{
				"function": "powerline.segments.common.time.date",
				"name": "time",
				"args": {
					"format": "%H:%M",
					"istime": true
				}
			}
		]
	}
}
